{{> nav}}


<div class="row" id="main">
    <div class="col-md-12">
        <p>ยอดรวม {{ users.length }} คน</p>
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>ลำดับ</th>
                    <th>ภาพ</th>
                    <th>ชื่อ - นามสกุล</th>
                    <th>Email</th>
                    <th>อายุ</th>
                    <th>เพศ</th>
                    <th>สนใจเพศ</th>
                    <th>ช่วงอายุ</th>
                    <th>จำนวนแชร์</th>
                    <th>Match</th>
                    
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                    <tr>
                        <td>{{rank @index}}</td>
                        <td><img src="{{profile_url}}" alt="" style="width: 200px;"></td>
                        <td>{{ fullname }}</td>
                        <td>{{ email }}</td>
                        <td>{{ age }}</td>
                        <td>{{ sex }}</td>
                        <td>{{ interest }}</td>
                        <td>{{ range_min }} - {{ range_max }}</td>
                        <td>{{ shared }}</td>
                        <td><a href="/mdata/user/match/{{facebookid}}">ดูข้อมูล</a></td>
                        
                        
                    </tr>
                {{/each}}
                
            </tbody>

        </table>
        
    </div>
</div>